{
  "name": "fastify-api",
  "version": "1.0.0",
  "description": "Fastify API with Prisma as the ORM and SQlite as the database",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "populate": "npx tsx scripts/populate_db.ts",
    "start": "node dist/src/server.js",
    "dev": "npx tsx src/server.ts",
    "start-dev": "npx nodemon --legacy-watch --signal SIGTERM dist/src/server.js",
    "build-dev": "tsc --watch",
    "test": "vitest",
    "test-db": "npx tsx scripts/test_db.ts",
    "tournament": "node dist/scripts/simulateTournament.js",
    "open-tournament": "node dist/scripts/openTournament.js",
    "login-tournament": "node dist/scripts/autoLogin.js",
    "db-reset": "npx prisma migrate reset"
  },
  "prisma": {
    "seed": "npx tsx scripts/populate_db.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/jwt": "^9.0.4",
    "@fastify/multipart": "^9.0.3",
    "@fastify/websocket": "^11.0.2",
    "@prisma/client": "^6.12.0",
    "async-mutex": "^0.5.0",
    "dotenv": "^16.4.7",
    "ethers": "^6.14.0",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "file-type": "^21.0.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@faker-js/faker": "^9.5.1",
    "@types/node": "^22.13.9",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/ws": "^8.18.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "ethers-types": "^3.18.1",
    "firefox": "^0.0.1",
    "nodemon": "^3.1.9",
    "playwright": "^1.54.1",
    "playwright-firefox": "^1.54.1",
    "prettier": "^3.5.3",
    "prisma": "^6.12.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.9"
  }
}
